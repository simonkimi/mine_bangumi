{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "src/**/*.d.ts",
    "auto-imports.d.ts",
    "components.d.ts"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@": [
        "./"
      ]
    },
    "jsx": "preserve",
    "jsxImportSource": "vue"
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
